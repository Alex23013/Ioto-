# Ethics Module
#####################################

  :RecommendationAttribute rdf:type owl:Class ;
      rdfs:subClassOf skos:Concept ;
      rdfs:isDefinedBy colpri: .

  :Recommendation rdf:type owl:Class ;
    :subClassOf owl:Thing ;
    :hasRecommendationAttribute :RecommendationAttribute ;
    :recommends :PrivacyPolicy ;
    rdfs:isDefinedBy colpri: .

  :hasRecommendationAttribute a owl:ObjectProperty ;
    rdfs:domain :Recommendation;
    rdfs:range :RecommendationAttribute ;
    rdfs:isDefinedBy colpri: .

  :recommends a owl:ObjectProperty ; #
      rdfs:domain :Recommendation;
      rdfs:range :PrivacyPolicy ;
      rdfs:isDefinedBy colpri: .
  
  :hasPrivacyPolicyEvaluationAttribute a owl:ObjectProperty ;
      rdfs:domain :PrivacyPolicyEvaluation;
      rdfs:range :PrivacyPolicyEvaluationAttribute ;
      rdfs:isDefinedBy colpri: .

  :PrivacyPolicyEvaluation rdf:type owl:Class ;
      rdfs:subClassOf seas:Evaluation ;
      :evaluates :PrivacyPolicy ;
      :hasPrivacyPolicyEvaluationAttribute colpri:PrivacyPolicyEvaluationAttribute ;
      rdfs:isDefinedBy colpri: .
  
  :PrivacyPolicyEvaluationAttribute rdf:type owl:Class ;
      rdfs:subClassOf skos:Concept ;
      rdfs:isDefinedBy colpri: .

  :PrivacyCommunityMember rdf:type owl:Class ;
      rdfs:subClassOf skos:Actor ;
      :givesPrivacyPolicyEvaluation :PrivacyPolicyEvaluation ;
      :givesRecommendation :Recommendation ;
      rdfs:isDefinedBy colpri: .
  
  :Administrator rdf:type owl:Class ;
      rdfs:subClassOf :PrivacyCommunityMember ;
      :validatesPrivacyPolicyEvaluation :PrivacyPolicyEvaluation ;
      :validatesRecommendation :Recommendation ;
      :givesRecommendation :Recommendation ;
      :givesPrivacyPolicyEvaluation :PrivacyPolicyEvaluation ;
      rdfs:isDefinedBy colpri: .

  :Contributor rdf:type owl:Class ;
      rdfs:subClassOf :PrivacyCommunityMember;
      :givesRecommendation :Recommendation ;
      :givesPrivacyPolicyEvaluation :PrivacyPolicyEvaluation ;
      rdfs:isDefinedBy colpri: .

  :givesRecommendation a owl:ObjectProperty ;
      rdfs:domain :PrivacyCommunityMember;
      rdfs:range :Recommendation ;
      rdfs:isDefinedBy colpri: .
  
  :givesPrivacyPolicyEvaluation a owl:ObjectProperty ;
      rdfs:domain :PrivacyCommunityMember;
      rdfs:range :PrivacyPolicyEvaluation;
      rdfs:isDefinedBy colpri: .

  :Purpose rdf:type owl:Class ;
      rdfs:subClassOf skos:Concept ;
      rdfs:subClassOf :PrivacyAttribute ;
      rdfs:isDefinedBy colpri: .
  
  :Advertising rdf:type owl:Class ;
      rdfs:subClassOf :Purpose ;
      rdfs:isDefinedBy colpri: .

  :ApplicationFunctionning rdf:type owl:Class ;
      rdfs:subClassOf :Purpose ;
      rdfs:isDefinedBy colpri: .

  :validatesPrivacyPolicyEvaluation a owl:ObjectProperty ;
      rdfs:domain :Administrator;
      rdfs:range :PrivacyPolicyEvaluation ;
      rdfs:isDefinedBy colpri: .

  :validatesRecommendation a owl:ObjectProperty ;
      rdfs:domain :Administrator;
      rdfs:range :Recommendation ;
      rdfs:isDefinedBy colpri: .
  
  ### Enon4IOT-addition

  :DataController rdf:type owl:Class ;
      rdfs:subClassOf :PrivacyCommunityMember ;
      rdfs:subClassOf [
          rdf:type owl:Restriction ;
          owl:onProperty :definesPurpose ;
          owl:minCardinality 1
      ] ;
      rdfs:isDefinedBy ioto: .
  
  :definesPurpose a owl:ObjectProperty ;
      rdfs:domain :DataController;
      rdfs:range :Purpose;
      rdfs:isDefinedBy ioto: .
  
  :thingToTakeResponsabilityFor rdf:type owl:Class ;
    rdfs:subClassOf owl:Thing ;
    owl:unionOf ( :Recommendation :PrivacyPolicyEvaluation :Disclosure :Consent ) ;
    rdfs:isDefinedBy ioto: .
  
  :stepToTakeResponsabilityFor rdf:type owl:Class ;
    rdfs:subClassOf skos:Concept ;
    owl:unionOf ( :DataTransfer :DataStoring :DataProcessing :DataCollection :DataControl :DataArchiving :DataDeleting ) ;
    rdfs:isDefinedBy ioto: .

  :takesResposabilityForThing a owl:ObjectProperty ;
    rdfs:domain :DataController ;
    rdfs:range :thingToTakeResponsabilityFor ;
    rdfs:isDefinedBy ioto: .

  :takesResposabilityForStep a owl:ObjectProperty ;
    rdfs:domain :DataController ;
    rdfs:range :stepToTakeResponsabilityFor ;
    rdfs:isDefinedBy ioto: .
  
  :makesDisclosure rdf:type owl:ObjectProperty ;
    rdfs:domain :PrivacyCommunityMember ;
    rdfs:range :Disclosure ;
    rdfs:isDefinedBy ioto: .
  
  :makesConsent rdf:type owl:ObjectProperty ;
    rdfs:domain :PrivacyCommunityMember ;
    rdfs:range :Consent ;
    rdfs:isDefinedBy ioto: .
  
  :definesPurpose a owl:ObjectProperty ;
    rdfs:domain :DataController ;
    rdfs:range :Purpose ;
    rdfs:isDefinedBy ioto: .

  :EthicCategory rdf:type owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:isDefinedBy ioto: .
  
  :Forbidden rdf:type owl:Class ;
    rdfs:subClassOf :EthicCategory ;
    rdfs:isDefinedBy ioto: .
  
  :Disliked rdf:type owl:Class ;
    rdfs:subClassOf :EthicCategory ;
    rdfs:isDefinedBy ioto: .

  :Permissible rdf:type owl:Class ;
    rdfs:subClassOf :EthicCategory ;
    rdfs:isDefinedBy ioto: .

  :Recommended rdf:type owl:Class ;
    rdfs:subClassOf :EthicCategory ;
    rdfs:isDefinedBy ioto: .
  
  :Mandatory rdf:type owl:Class ;
    rdfs:subClassOf :EthicCategory ;
    rdfs:isDefinedBy ioto: .

  :hasEthicCategory rdf:type owl:ObjectProperty ;
    rdfs:domain :DataController ;
    rdfs:range :EthicCategory ;
    rdfs:isDefinedBy ioto: .

    [ rdf:type owl:Class ;
      owl:intersectionOf ( :DataController
        [ rdf:type owl:Restriction ;
          owl:onProperty :definesPurpose ;
          owl:someValuesFrom :Purpose
        ]
      )
    ] rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :hasEthicCategory ;
      owl:hasValue :Mandatory
    ] .

  :EthicMode rdf:type owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:isDefinedBy ioto: .
  
  :Mild rdf:type owl:Class ;
    rdfs:subClassOf :EthicMode ;
    rdfs:isDefinedBy ioto: .
  
  :Default rdf:type owl:Class ;
    rdfs:subClassOf :EthicMode ;
    rdfs:isDefinedBy ioto: .

  :Strong rdf:type owl:Class ;
    rdfs:subClassOf :EthicMode ;
    rdfs:isDefinedBy ioto: .

  :Stringent rdf:type owl:Class ;
    rdfs:subClassOf :EthicMode ;
    rdfs:isDefinedBy ioto: .
